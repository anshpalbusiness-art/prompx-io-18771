# Whop Configuration

# Get these values from your Whop dashboard at https://whop.com/dashboard
# After creating your products, replace these placeholder values

# Whop API Key (from Settings > API Keys)
VITE_WHOP_API_KEY=your_whop_api_key_here

# Product IDs for each plan (from Products section)
# Format: prod_xxxxxxxxxxxxx

# Pro Plan Product IDs
VITE_WHOP_PRO_MONTHLY_ID=prod_pro_monthly_id_here
VITE_WHOP_PRO_YEARLY_ID=prod_pro_yearly_id_here

# Premium Plan Product IDs
VITE_WHOP_PREMIUM_MONTHLY_ID=prod_premium_monthly_id_here
VITE_WHOP_PREMIUM_YEARLY_ID=prod_premium_yearly_id_here

# Whop Webhook Secret (from Settings > Webhooks)
WHOP_WEBHOOK_SECRET=whop_webhook_secret_here

# Return URLs (update with your production domain)
VITE_WHOP_SUCCESS_URL=http://localhost:8081/payment/success
VITE_WHOP_CANCEL_URL=http://localhost:8081/payment/cancel
